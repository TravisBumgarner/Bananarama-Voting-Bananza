{
    "name": "backend",
    "author": "Travis Bumgarner",
    "version": "1.0.0",
    "scripts": {
        "sd:be": "export NODE_ENV=local && ts-node-dev ./index.ts --ignore-watch node_modules",
        "build": "rm -rf ./build && tsc",
        "start": "node build/index.js",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./src/db/ormconfig.ts",
        "migration:generate": "export NODE_ENV=local && yarn run typeorm migration:generate -- -n",
        "migration:run": "yarn run typeorm migration:run",
        "migration:revert": "yarn run typeorm migration:revert",
        "migration:run:dev": "export NODE_ENV=local && yarn run migration:run",
        "migration:run:docker": "export NODE_ENV=local && docker exec -it placeaday-backend yarn run migration:run:dev",
        "migration:generate:docker": "docker exec -it placeaday-backend yarn run migration:generate",
        "migration:revert:docker": "export NODE_ENV=local && docker exec -it placeaday-backend yarn run migration:revert",
        "precommit": "echo \"---------------------------- NOTHING TO SEE HERE ON BACKEND ----------------------------\"",
        "deploy:production": "export NODE_ENV=production && yarn run build && gcloud config set project placeaday-fd46a && gcloud builds submit --tag gcr.io/placeaday-fd46a/backend && gcloud run deploy backend --image gcr.io/placeaday-fd46a/backend --platform managed --region us-central1",
        "lint:run": "npx eslint --ext .ts ./src/**",
        "lint:fix": "npx eslint --ext .ts ./src/** --fix",
        "migration:run:production": "export NODE_ENV=production IS_RUNNING_MIGRATIONS=true && yarn run migration:run",
        "cloudsql:production": "gcloud config set project placeaday-fd46a && ./cloud_sql_proxy -instances=placeaday-fd46a:us-central1:placeaday=tcp:0.0.0.0:1234"
    },
    "keywords": [],
    "license": "ISC",
    "dependencies": {
        "@sentry/node": "^7.21.1",
        "@sentry/tracing": "^7.21.1",
        "body-parser": "^1.19.2",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.2",
        "express-graphql": "^0.12.0",
        "firebase-admin": "^10.0.2",
        "graphql": "^16.3.0",
        "graphql-redis-subscriptions": "^2.5.0",
        "graphql-ws": "^5.11.2",
        "pg": "^8.7.3",
        "redis": "^4.5.1",
        "runtypes": "^6.5.0",
        "ts-node": "^10.4.0",
        "typeorm": "^0.2.41",
        "uuid": "^8.3.2",
        "ws": "^8.11.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.17",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "eslint": "^8.10.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.25.4",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.8.4"
    }
}
